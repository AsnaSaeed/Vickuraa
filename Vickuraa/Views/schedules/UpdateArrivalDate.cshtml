@model Vickuraa.Models.schedule

<h1><span class="text-muted font-weight-light"><i class="page-header-icon ion-ios-clock success"></i>Update Arrival Date</span></h1>





@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.scheduleID)



        <div class="form-group">
            @Html.LabelFor(model => model.arrivedDate, "Arrived Date", htmlAttributes: new { @class = "control-label" })
            @Html.EditorFor(model => model.arrivedDate, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.arrivedDate, "", new { @class = "text-danger" })
        </div>



        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-warning" />
            </div>
        </div>
    </div>
}