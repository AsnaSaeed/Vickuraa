@model IEnumerable<Vickuraa.Models.vessel>


<h1><span class="text-muted font-weight-light"><i class="page-header-icon ion-ios-gear"></i>Vessel List </span></h1>



<div class="table-primary">
    <div id="datatables_wrapper" class="dataTables_wrapper form-inline dt-bootstrap no-footer">
        <div class="table-header clearfix"><div class="table-caption">Some header text</div><div class="DT-lf-right"><div class="DT-per-page"><div class="dataTables_length" id="datatables_length"><label>Per page: <select name="datatables_length" aria-controls="datatables" class="form-control input-sm"><option value="10">10</option><option value="25">25</option><option value="50">50</option><option value="100">100</option></select></label></div></div><div class="DT-search"><div id="datatables_filter" class="dataTables_filter"><label><input type="search" class="form-control input-sm" placeholder="Search..." aria-controls="datatables"></label></div></div></div></div><div class="dataTables_table_wrapper">
            <table class="table table-striped table-bordered dataTable no-footer" id="datatables" role="grid" aria-describedby="datatables_info">
                <thead>
                    <tr role="row"><th class="sorting_asc" tabindex="0" aria-controls="datatables" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Rendering engine: activate to sort column descending" style="width: 211.6px;">Rendering engine</th><th class="sorting" tabindex="0" aria-controls="datatables" rowspan="1" colspan="1" aria-label="Browser: activate to sort column ascending" style="width: 314.8px;">Browser</th><th class="sorting" tabindex="0" aria-controls="datatables" rowspan="1" colspan="1" aria-label="Platform(s): activate to sort column ascending" style="width: 285.2px;">Platform(s)</th><th class="sorting" tabindex="0" aria-controls="datatables" rowspan="1" colspan="1" aria-label="Engine version: activate to sort column ascending" style="width: 178.8px;">Engine version</th><th class="sorting" tabindex="0" aria-controls="datatables" rowspan="1" colspan="1" aria-label="CSS grade: activate to sort column ascending" style="width: 126px;">CSS grade</th></tr>
                </thead>
                <tbody>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.user.username)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.VesselRegNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.availWeight)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.availVolume)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.vesselName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.enteredDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.enteredUser)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr class="gradeA odd" role="row">
                            <td class="sorting_1">
                                @Html.DisplayFor(modelItem => item.user.username)
                            </td>
                            <td class="sorting_1">
                                @Html.DisplayFor(modelItem => item.VesselRegNo)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.availWeight)
                            </td>
                            <td class="sorting_1">
                                @Html.DisplayFor(modelItem => item.availVolume)
                            </td>
                            <td class="sorting_1">
                                @Html.DisplayFor(modelItem => item.vesselName)
                            </td>
                            <td class="sorting_1">
                                @Html.DisplayFor(modelItem => item.enteredDate)
                            </td>
                            <td class="sorting_1">
                                @Html.DisplayFor(modelItem => item.enteredUser)
                            </td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id = item.vesselID }) |
                                @Html.ActionLink("Details", "Details", new { id = item.vesselID }) |
                                @Html.ActionLink("Delete", "Delete", new { id = item.vesselID })
                            </td>
                        </tr>
                    }

            </table>

        </div><div></div><div class="table-footer clearfix"><div class="DT-label"><div class="dataTables_info" id="datatables_info" role="status" aria-live="polite">Showing 1 to 10 of 57 entries</div></div><div class="DT-pagination"><div class="dataTables_paginate paging_simple_numbers" id="datatables_paginate"><ul class="pagination"><li class="paginate_button previous disabled" id="datatables_previous"><a href="#" aria-controls="datatables" data-dt-idx="0" tabindex="0">Previous</a></li><li class="paginate_button active"><a href="#" aria-controls="datatables" data-dt-idx="1" tabindex="0">1</a></li><li class="paginate_button "><a href="#" aria-controls="datatables" data-dt-idx="2" tabindex="0">2</a></li><li class="paginate_button "><a href="#" aria-controls="datatables" data-dt-idx="3" tabindex="0">3</a></li><li class="paginate_button "><a href="#" aria-controls="datatables" data-dt-idx="4" tabindex="0">4</a></li><li class="paginate_button "><a href="#" aria-controls="datatables" data-dt-idx="5" tabindex="0">5</a></li><li class="paginate_button "><a href="#" aria-controls="datatables" data-dt-idx="6" tabindex="0">6</a></li><li class="paginate_button next" id="datatables_next"><a href="#" aria-controls="datatables" data-dt-idx="7" tabindex="0">Next</a></li></ul></div></div></div>
    </div>
</div>







